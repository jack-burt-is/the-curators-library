config_version=5

[application]

config/name="The Curator's Library"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://sounds/default_bus_layout.tres"

[autoload]

InteractionManager="*res://scenes/singletons/interaction_manager.tscn"
InputHelper="*res://addons/input_helper/input_helper.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
GameManager="*res://scripts/singletons/game_manager.gd"
SceneTransition="*res://scenes/singletons/scene_transition.tscn"

[dialogic]

directories/dch_directory={
"josephine": "res://dialogue/characters/josephine.dch",
"oliver": "res://dialogue/characters/oliver.dch"
}
directories/dtl_directory={
"josephine": "res://dialogue/timelines/josephine.dtl",
"oliver": "res://dialogue/timelines/oliver.dtl"
}
layout/style_list=["res://dialogue/styles/bubble.tres"]
layout/default_style="res://dialogue/styles/bubble.tres"
glossary/default_case_sensitive=true
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.03
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
variables={
"mood": "neutral"
}
text/input_action="dialogic_default_action"
glossary/glossary_files=[]

[dialogue_manager]

general/balloon_path="res://scenes/balloon.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[dotnet]

project/assembly_name="The Curator-s Library"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/input_helper/plugin.cfg")

[global_group]

player=""
pause_menu=""

